*.foreground:   $element.foreground
*.background:   $element.background
*.cursorColor:  $element.foreground

! black
*.color0:       $color.00
*.color8:       $color.08

! red
*.color1:       $color.01
*.color9:       $color.09

! green
*.color2:       $color.02
*.color10:      $color.10

! yellow
*.color3:       $color.03
*.color11:      $color.11

! blue
*.color4:       $color.04
*.color12:      $color.12

! magenta
*.color5:       $color.05
*.color13:      $color.13

! cyan
*.color6:       $color.06
